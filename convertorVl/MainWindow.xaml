<Window x:Class="convertorVl.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:convertorVl"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="500">
    <StackPanel Margin="8">
        <Label Content="Конвертер валют" />
        <Label Content="Из: "  />
        <ComboBox Name="FromComboBox"   />
        <Label Content="В: "  />
        <ComboBox Name="ToComboBox"    />
        <Label Margin="0 50 0 0" Content="Исходная величина:"  />
        <TextBox Name="TextBox1" TextChanged="TextBox1_TextChanged_1"  />
        <Label Content="Результат"  />
        <TextBox IsReadOnly="True" Name="RezutTextBox" />
    </StackPanel>
</Window>
